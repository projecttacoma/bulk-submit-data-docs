  fhir:OperationDefinition.url [ fhir:value "https://projecttacoma.github.io/bulk-submit-data-docs/OperationDefinition/Operation-measure-bulk-submit-data"];
  fhir:OperationDefinition.version [ fhir:value "4.0.1"];
  fhir:OperationDefinition.name [ fhir:value "Bulk Submit Data"];
  fhir:OperationDefinition.status [ fhir:value "draft"];
  fhir:OperationDefinition.kind [ fhir:value "operation"];
  fhir:OperationDefinition.date [ fhir:value "2022-02-25T10:52:40-05:00"^^xsd:dateTime];
  fhir:OperationDefinition.publisher [ fhir:value "The MITRE Corporation"];
  fhir:OperationDefinition.description [ fhir:value "The bulk submit-data operation completes a bulk import request based on the data requirements of a FHIR Measure. As with standard [$submit-data requests](http://hl7.org/fhir/R4/operation-measure-submit-data.html), the submitted data must include one MeasureReport resource, and the requests are kicked off using the $submit-data endpoint. However, for a bulk submit-data request, the MeasureReport references the measure-of-interest by canonical URL. It is assumed that the data-of-interest for the measure-of-interest is already stored on the server. If the “prefer”: “respond-async” request header is provided, the data requirements will be retrieved for the measure-of-interest, and a modified bulk import request will kick off using the $import endpoint to import the data that fulfills the data requirements. \n\nRequired header(s):\n```\n'prefer': 'respond-async'\n```"];
  fhir:OperationDefinition.code [ fhir:value "submit-data"];
  fhir:OperationDefinition.comment [ fhir:value "The expected responses from the bulk submit-data operation match the expected responses for the standard [bulk import operation](https://github.com/smart-on-fhir/bulk-import/blob/master/import-pnp.md)."];
