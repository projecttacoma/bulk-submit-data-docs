     fhir:OperationDefinition.parameter.min [ fhir:value "1"^^xsd:integer ];
     fhir:OperationDefinition.parameter.max [ fhir:value "1" ];
     fhir:OperationDefinition.parameter.documentation [ fhir:value "The measure report being submitted, which references the measure-of-interest by canonical URL" ];
     fhir:OperationDefinition.parameter.type [ fhir:value "MeasureReport" ]
  ], [
